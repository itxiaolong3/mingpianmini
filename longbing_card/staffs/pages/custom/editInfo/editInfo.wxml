<block wx:if='{{showClientSource == true}}'><view class='bg_opacity_sec fix' style='z-index:99;'></view></block><view class='showUseMessage {{(showClientSource == true?"choose-animatoinsA":"choose-animatoinsB")}}' style='height:390rpx;'><view class='title'>客户来源<i bindtap='toJump' class='iconfont icon-guanbi1' data-status='toClose'></i></view><view class='content' style='height:291rpx;'><view class='source'><block wx:if='{{showClientSourceData.clientSourceType == "group"}}'>{{showClientSourceData.clientSourceStr[0]}}<block wx:if='{{clientData.openGId}}'><open-data openGid='{{clientData.openGId}}' type='groupName'></open-data></block>{{showClientSourceData.clientSourceStr[1]}}</block><block wx:else>{{clientData.share_str}}</block></view></view></view><form bindsubmit='formSubmit' reportSubmit='true'><view class='staff-editInfo-sec'><view class='child'><view bindtap='toJump' class='flex' data-status='toSource'><view class='flex100-2'>来源</view><view class='flex100-8 tr'>点击查看<i class='iconfont icon-right2 fr' style='padding-left:10rpx;'></i></view></view></view><view class='child'><view class='flex'><view class='flex100-2'>姓名</view><view class='flex100-8 tr'><block wx:if='{{param.fromstatus == "boss"}}'><view class='flex100-8 tr'>{{(clientData.info.name?clientData.info.name:(clientData.info.nickName?clientData.info.nickName:"暂未填写"))}}</view></block><block wx:else><input name='name' placeholder='姓名' type='text' value='{{(clientData.info.name?clientData.info.name:clientData.info.nickName)}}'></input></block></view></view></view><view class='child'><view class='flex'><view class='flex100-2'>性别</view><view class='flex100-8 tr'><block wx:if='{{param.fromstatus == "boss"}}'><view class='flex100-8 tr'>{{(setCount[count].name?setCount[count].name:"暂未设置")}}</view></block><block wx:else><i class='iconfont icon-right2 fr'></i><picker bindchange='pickerSelected' range='{{setCount}}' rangeKey='{{"name"}}' value='{{count}}'>              {{(setCount[count].name?setCount[count].name:"请选择")}}            </picker></block></view></view></view><view class='child'><view class='flex'><view class='flex100-2'>手机号</view><view class='flex100-8 tr'><block wx:if='{{param.fromstatus == "boss"}}'><view class='flex100-8 tr'>{{(clientData.info.phone?clientData.info.phone:"暂未填写")}}</view></block><block wx:else><input name='phone' placeholder='电话' type='text' value='{{clientData.info.phone}}'></input></block></view></view></view><view class='child'><view class='flex'><view class='flex100-2'>邮箱</view><view class='flex100-8 tr'><block wx:if='{{param.fromstatus == "boss"}}'><view class='flex100-8 tr'>{{(clientData.info.email?clientData.info.email:"暂未填写")}}</view></block><block wx:else><input name='email' placeholder='邮箱' type='text' value='{{clientData.info.email}}'></input></block></view></view></view><view class='child'><view class='flex'><view class='flex100-2'>公司</view><view class='flex100-8 tr'><block wx:if='{{param.fromstatus == "boss"}}'><view class='flex100-8 tr'>{{(clientData.info.company?clientData.info.company:"暂未填写")}}</view></block><block wx:else><input name='company' placeholder='公司' type='text' value='{{clientData.info.company}}'></input></block></view></view></view><view class='child'><view class='flex'><view class='flex100-2'>职称</view><view class='flex100-8 tr'><block wx:if='{{param.fromstatus == "boss"}}'><view class='flex100-8 tr'>{{(clientData.info.position?clientData.info.position:"暂未填写")}}</view></block><block wx:else><input name='position' placeholder='职称' type='text' value='{{clientData.info.position}}'></input></block></view></view></view><view class='child'><view class='flex'><view class='flex100-2'>详细地址</view><view class='flex100-8 tr'><block wx:if='{{param.fromstatus == "boss"}}'><view class='flex100-8 tr'>{{(clientData.info.address?clientData.info.address:"暂未填写")}}</view></block><block wx:else><input name='address' placeholder='详情地址' type='text' value='{{clientData.info.address}}'></input></block></view></view></view><view class='child'><view class='flex'><view class='flex100-2'>生日</view><view class='flex100-8 tr'><block wx:if='{{param.fromstatus == "boss"}}'><view class='flex100-8 tr'>{{(date?date:"暂未设置")}}</view></block><block wx:else><i class='iconfont icon-right2 fr'></i><picker bindchange='listenerDatePickerSelected' mode='date' value='{{date}}'>              {{(date?date:"请选择")}}            </picker></block></view></view></view><view class='child'><view class='flex'><view class='flex100-2' style='flex:0 0 50%;'>屏蔽Ta的消息推送</view><view class='flex100-8 tr' style='flex:0 0 50%;'><switch bindchange='switchChange' checked='{{(clientData.info.is_mask == 1?"checked":"")}}' name='is_mask' style='zoom:0.7'></switch></view></view></view></view><view class='staff-editInfo-common'><view class='title'>备注</view><block wx:if='{{param.fromstatus == "boss"}}'><view class='remark'>{{(clientData.info.remark?clientData.info.remark:"暂未备注")}}</view></block><block wx:else><input class='textarea br-15' maxlength='-1' name='remark' placeholder='备注' style='height:80rpx;min-height:80rpx;line-height:80rpx;' type='text' value='{{clientData.info.remark}}'></input></block></view><block wx:if='{{!(param.fromstatus)}}'><button class='fix-painter-share tc' data-status='toEditStaff' formType='submit' hoverClass='none' style='border-radius:15rpx;width:92%;margin:60rpx auto;background:#e93636;'>    保存  </button></block></form><view class='{{(globalData.isIphoneX?"isIphoneX":"")}}'></view>