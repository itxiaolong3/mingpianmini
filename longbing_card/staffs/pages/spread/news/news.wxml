<import src='../../../../templates/copyright/copyright.wxml'></import><block wx:if='dataList.length>0}}'><view class='spread-news-sec'><block wx:for='{{dataList}}' wx:for-item='item' wx:for-index='index' wx:key='key'><view class='child rel'><view bindtap='toJump' class='title clamp2' data-id='{{item.id}}' data-status='toNewsDetail'>                {{item.title}}            </view><block wx:if='{{item.cover.length > 0}}'><block wx:if='{{status == "news"}}'><view bindtap='toJump' class='imgs-sec clearfix' data-id='{{item.id}}' data-status='toNewsDetail'><block wx:for='{{item.cover}}' wx:for-item='item1' wx:for-index='index1' wx:key='key1'><block wx:if='{{index1 < 3}}'><image class='fl br-5' mode='aspectFill' src='{{item1}}'></image></block></block></view></block></block><view class='flex'><view class='flex100-4'>{{item.create_time}}</view><form bindsubmit='formSubmit' class='flex100-6 tr' reportSubmit='true'><button class='fr delete ml-20' data-index='{{index}}' data-status='toDelete' formType='submit' hoverClass='none'>                        删除                    </button><block wx:if='{{status == "news"}}'><button class='fr share' data-id='{{item.id}}' data-index='{{index}}' data-status='toShare' formType='submit' hoverClass='none' openType='share'>                        转发                    </button></block><block wx:if='{{status == "code"}}'><button class='fr share' data-index='{{index}}' data-status='toCodeDetial' formType='submit' hoverClass='none'>                        查看                    </button></block></form></view></view></block></view></block><block wx:if='{{show}}'><include src='/longbing_card/templates/loading.wxml'></include></block><template is='copyright' data='{{globalData:globalData}}'></template><view class='{{(globalData.isIphoneX?"isIphoneX":"")}}' style='height:90rpx;'></view><view bindtap='toJump' class='fix-painter-share fix {{(globalData.isIphoneX?"isIphoneX":"")}}' data-status='toAddNews' data-url='/longbing_card/staffs/pages/spread/news/addNews/addNews?status={{status}}' style='background:white;'><button style='background:#fc3c3b;'><block wx:if='{{status == "news"}}'>新建动态</block><block wx:if='{{status == "code"}}'>新建自定义码</block></button></view><view class='{{(globalData.isIphoneX?"isIphoneX":"")}}' style='height:100rpx;'></view>