page{ background: #f4f4f8; }
wx-view,wx-button{ display: block; overflow: initial; }
.card-product-sec .child{ height: auto; }
.card-product-sec .child .content .title{ font-size: 30rpx; line-height:39rpx; height: 78rpx; margin:10rpx 0rpx 25rpx 0rpx; }
.card-product-sec .child .content .flex .flex100-6{ line-height: 50rpx; font-size: 26rpx; color: #979797; }
.form-btn-sec{ width: 100%; height: 50rpx; line-height: 50rpx; padding-top: 30rpx; display: block; margin-top: 15rpx; border-top: 1rpx solid #f1f1f1; }
.form-btn-sec wx-button{ height: 50rpx; line-height: 50rpx; display: flex; align-items: center; justify-content: center; border-radius: 6rpx; font-size: 26rpx; }
.form-btn-sec wx-button.allCheck{ background: #4877bc; color: white; border: 1rpx solid #4877bc; }
.form-btn-sec wx-button.check{ background: white; color: #4877bc; border: 1rpx solid #4877bc; }
.form-btn-sec wx-button.allPush{ background: #f31a33; color: white; border: 1rpx solid #f31a33; }
.form-btn-sec wx-button.push{ background: white; color: #f31a33; border: 1rpx solid #f31a33; }
.form-btn-sec wx-button.share{ background: white; color: #ff9600; border: 1rpx solid #ff9600; }
