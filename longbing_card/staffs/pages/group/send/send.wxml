<block wx:if='{{paramObj.status == "message"}}'><view class='staff-group-send-sec'><view class='text'>你将发消息给{{paramObj.count}}个标签组</view><view class='tags'>{{paramObj.lable_name}}</view></view></block><import src='../../../../chat/chat-input/chat-input.wxml'></import><import src='../../../../templates/loading/loading.wxml'></import><view bindtap='hideExtra' class='speak_box'><block wx:for='{{dataList.list}}' wx:for-item='item' wx:for-index='index' wx:key='key'><view class='tc'><view class='last_time2'>{{item.last_time2}}</view></view><view class='answer'><view class='heard_img left'><image src='https://retail.xiaochengxucms.com/images/12/2018/11/uO4Ru42M720LSZa70aRA4laoANRx44.png'></image></view><view class='answer_text'><view bindtap='toJump' class='p' data-status='toCopy' data-url='_COPY_DEEP_XX{{item.content}}'>{{item.content}}</view><i></i></view></view></block></view><block wx:if='{{paramObj.status == "manager"}}'><template is='loading' data='{{...dataList,loading:dataList.loading}}'></template></block><block wx:if='{{paramObj.status == "message"}}'><view class='new-chat-sec {{(globalData.isIphoneX?"isIphoneX":"")}}' style='bottom:{{(inputObj.extraObj.chatInputShowExtra?378:120)}}rpx;'><view class='staff'><view class='use'><text bindtap='toJump' data-status='toUse'>常用话术</text></view></view></view><view class='showUseMessage {{(showUseMessage == true?"":"hide")}}'><view class='title'>常用话术<i bindtap='toJump' class='iconfont icon-guanbi1' data-status='toClose'></i></view><view class='content'><view class='nav-left'><block wx:for='{{useMessageType}}' wx:for-item='item' wx:for-index='index' wx:key='key'><view bindtap='toJump' class='child {{(currUType == index?"active":"")}}' data-index='{{index}}' data-status='toSetTab'>{{item}}</view></block></view><view class='nav-right'><block wx:if='{{currUType == 0}}'><view bindtap='toJump' class='add-useMessage-sec' data-status='toAdd' style='margin-left:7%;'><i class='iconfont icon-jia'></i>新增话术</view></block><block wx:if='{{currUType == 0}}'><view bindtap='toJump' class='add-useMessage-sec' data-status='toEditSec' data-type='{{showEditSec}}'><i class='iconfont icon-jilu'></i>编辑话术</view></block><view style='clear:both;'></view><block wx:for='{{useMessage[currUType].list}}' wx:for-item='item' wx:for-index='index' wx:key='key'><view class='child'><view bindtap='toJump' class='text' data-content='{{item.content}}' data-index='{{index}}' data-status='toSendMessage'>{{item.content}}</view><view class='line'></view><block wx:if='{{ (currUType == 0 && showEditSec == true) }}'><view bindtap='toJump' class='edit' data-content='{{item.content}}' data-index='{{index}}' data-status='toEdit'><i class='iconfont icon-jilu'></i> 修改</view><view bindtap='toJump' class='delete' data-index='{{index}}' data-status='toDelete'><i class='iconfont icon-shanchu-01'></i> 删除</view><view class='line'></view></block></view></block></view></view></view><block wx:if='{{showAddUseSec == true}}'><view class='bg_opacity_sec fix'></view></block><block wx:if='{{showAddUseSec == true}}'><form bindsubmit='formSubmit' class='add-useMessage-form' reportSubmit='true'><view class='title'>新建话术</view><textarea maxlength='-1' name='newuse' placeholder='请输入您的话术' type='text' value='{{showAddUseSecContent}}'></textarea><button class='btn cancel' data-status='toCancel' formType='submit' hoverClass='none'>取消</button><button class='btn' data-status='toSaveUseMessage' formType='submit' hoverClass='none'>确认</button></form></block><view style='height:{{(inputObj.extraObj.chatInputShowExtra?435:260)}}rpx;'></view><template is='chat-input' data='{{globalData:globalData,inputObj:inputObj,textMessage:textMessage,showVoicePart:false}}'></template></block>