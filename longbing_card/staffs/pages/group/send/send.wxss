.showUseMessage .content{ width: 100%; height: 691rpx; background: white; display: block; position: fixed; bottom: 0rpx; overflow: auto; }
.showUseMessage .content .source{ width: 90%; margin: 40rpx auto; font-size: 28rpx; color: #646464; height: 200rpx; overflow: auto; }
.showUseMessage .content .nav-left{ width: 28%; height: 691rpx; float: left; position: fixed; left: 0rpx; bottom: 0rpx; overflow: auto; background: #f7f7f7; }
.showUseMessage .content .nav-left .child{ width:84%; line-height: 40rpx; padding: 25rpx 8%; display: block; font-size: 26rpx; color: #646464; }
.showUseMessage .content .nav-left .child.active{ color: #ff0909; background: white; }
.showUseMessage .content .nav-right{ width: 72%; float: right; height: 692rpx; display: block; overflow-x: hidden; }
.add-useMessage-sec{ width: 40%; height: 84rpx; line-height: 84rpx; margin: 30rpx 3% 10rpx 3%; float: left; font-size: 28rpx; text-align: center; color: #ff0000; border: 1rpx solid #f0f0f0; border-radius: 42rpx; box-shadow: 0 3rpx 12rpx rgba(0, 0, 0, .1); }
