<view class='card-toSearch-sec fix'><block wx:if='{{toSearchCard == true}}'><input bindblur='toSearchCardBlur' bindconfirm='toSearchCardConfirm' bindinput='toSearchCard' class='br-10' placeholder='搜索标签' type='text' value='{{cardSearchKey}}'></input></block><block wx:else><view bindtap='toJump' class='input tc' data-status='toSearchCardFocus'><i class='iconfont icon-sousuo'></i>{{(cardSearchKey?cardSearchKey:"搜索标签")}}</view></block></view><view style='height:112rpx;'></view><view class='staff-group-tag-sec'><view class='title'>标签群发</view><block wx:for='{{dataList.list}}' wx:for-item='item' wx:for-index='index' wx:key='key'><view bindtap='toJump' class='child {{(tagType[index] == 1?"active":"")}}' data-index='{{index}}' data-status='toCheckTag' data-type='{{tagType[index]}}'>{{item.name}}({{item.count}})</view></block></view><view class='{{(globalData.isIphoneX?"isIphoneX":"")}}' style='height:90rpx;'></view><view bindtap='toJump' class='fix-painter-share fix {{(globalData.isIphoneX?"isIphoneX":"")}}' data-status='toJumpUrl' data-type='toSend' data-url='/longbing_card/staffs/pages/group/send/send?status=message&type=next' style='background:white;'><button style='background:#fc3c3b;'>下一步</button></view>