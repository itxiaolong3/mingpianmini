.member{
  text-align: center;
  height: 110rpx;
  line-height: 110rpx;
  color: #fff;
  background-color: #ff4544;
  margin-bottom: 20rpx;
}
.list{
  display: flex;
  justify-content: space-around;
  text-align: left;
  letter-spacing: 0rpx;
  height: 210rpx;
  margin-bottom: 25rpx;
   background-color: white;
  margin-top: 1px;
}
.list-left image{
   width: 160rpx;
  height: 160rpx;
  overflow: hidden;
  text-align: center;
  border-radius: 50%;
  margin-top: 20rpx;
}
.list-con{
 
   /* display: flex;
   flex-direction:column;
   justify-content: space-around; */
   height: 100rpx;
}
.list-con view:first-child{
  color: #ff8c12;
  line-height: 100rpx;
}
.list-con view:last-child{
  color: #8f8b8b;
}
.list-min{
  height: 210rpx;
  line-height: 210rpx;
  color: #484545;
}
.list-right{
  height: 210rpx;
  line-height:210rpx;
}
.list-right image{
  display: inline-block;
  width:80rpx;
  height: 80rpx;
   vertical-align:middle;
}