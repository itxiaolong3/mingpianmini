}
.min-top{
  background-color: #fc4f4e;
  color: #fff;
  margin-bottom: 25rpx;
}
.min-top view:first-child{
  height: 90rpx;
  line-height: 90rpx;
  
  /* color: #fff; */
}
.grade{
  display: flex;
  justify-content: space-around;
  align-items: center;
  border-top: 1px white solid;
  border-radius: 5px;
  margin-top: 10px;
}
.ischoose{
  color:#FFCA00;
}
.list{
  display: flex;
  justify-content: space-around;
