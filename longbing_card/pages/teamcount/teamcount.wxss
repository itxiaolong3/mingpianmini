  letter-spacing: 0rpx;
  height: 210rpx;
  margin-bottom: 25rpx;
  border-top: 1px white solid;
}
.list-left image{
   width: 160rpx;
  height: 160rpx;
  overflow: hidden;
  text-align: center;
  border-radius: 50%;
  margin-top: 20rpx;
}
.list-con view{
   /* display: flex;
