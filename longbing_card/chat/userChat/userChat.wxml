<import src='../chat-input/chat-input.wxml'></import><view class='fixed-right-sec fixed-shop-right-sec fix'><block wx:if='{{is_tpl == 1}}'><view bindtap='toJump' class='circle tc' data-method='reLaunch' data-status='toHome' data-url='/longbing_card/pages/index/index?to_uid={{chat_to_uid}}&from_id={{chat_to_uid}}&currentTabBar=toCard'><i class='iconfont icon-shouye'></i></view></block></view><block wx:if='{{isEmpty == true}}'><view class='speak_box'><view class='answer'><view class='heard_img left'><image src='{{toChatAvatarUrl}}'></image></view><view class='answer_text'><view class='oldqq p'><text>{{staffDefaultData.title}}</text><block wx:for='{{staffDefaultData.info}}' wx:for-item='item' wx:for-index='index' wx:key='key'><view bindtap='toJump' class='oldqq_container' data-index='{{index}}' data-status='toSeeStaff'><image src='{{item.img}}' style='padding:0 20rpx;'></image><text style='display:flex;flex:1;padding-left:20rpx;'>{{item.name}}</text><image src='/longbing_card/resource/images/img/0.png' style='width:18rpx;height:24rpx;padding:0 20rpx;'></image></view></block><block wx:if='{{staffDefaultData.phone}}'><view bindtap='toJump' class='oldqq_bottom' data-content='{{staffDefaultData.phone}}' data-status='toCallCopy' data-type='2'><text>拨打电话</text><image src='/longbing_card/resource/images/img/ph.png'></image></view></block><block wx:if='{{staffDefaultData.wechat}}'><view bindtap='toJump' class='oldqq_bottom' data-content='{{staffDefaultData.wechat}}' data-status='toCallCopy' data-type='4'><text>加我微信</text><image src='/longbing_card/resource/images/img/wx.png'></image></view></block></view><i></i></view></view></view></block><view bindtap='hideExtra' class='speak_box'><block wx:for='{{messageList}}' wx:for-item='item' wx:for-index='index' wx:key='key'><view class='line-text-time'>{{item.create_time}}</view><block wx:for='{{item.list}}' wx:for-item='item1' wx:for-index='index1' wx:key='key1'><view class='{{(item1.user_id == user_id?"question":"answer")}}'><view class='heard_img {{(item1.user_id == user_id?"right":"left")}}'><image src='{{(item1.user_id == user_id?chatAvatarUrl:toChatAvatarUrl)}}'></image></view><view class='{{(item1.user_id == user_id?"question_text clear":"answer_text")}}'><block wx:if='{{item1.type == "image"}}'><view bindtap='toJump' class='{{(item1.user_id == user_id?"question":"answer")}}_img' data-content='{{item1.content}}' data-status='previewImage'><image mode='widthFix' src='{{item1.content}}'></image></view></block><block wx:if='{{item1.type == "text"}}'><view bindtap='toJump' class='p' data-name='{{contactUserName}}' data-status='toCopy' data-url='_COPY_DEEP_XX{{item1.content}}'>{{item1.content}}</view><i></i></block></view></view></block></block></view><view class='new-chat-sec {{(globalData.isIphoneX?"isIphoneX":"")}}' style='bottom:{{(inputObj.extraObj.chatInputShowExtra?368:118)}}rpx;'><view class='user'>        我想<block wx:if='{{staffDefaultData.wechat}}'><text bindtap='toJump' data-content='{{staffDefaultData.wechat}}' data-status='toCopyWechat' data-type='4'>加微信</text></block><block wx:if='{{staffDefaultData.phone}}'><text bindtap='toJump' data-status='toCallPhone' data-type='2' data-url='tel:{{staffDefaultData.phone}}'>打电话</text></block><text bindtap='toJump' data-status='toSaveCard'>保存名片码</text></view></view><block wx:if='{{countMessage > 5}}'><block wx:if='{{globalData.isIphoneX == true}}'><view style='height:{{(inputObj.extraObj.chatInputShowExtra?518:270)}}rpx;'></view></block><block wx:else><view style='height:{{(inputObj.extraObj.chatInputShowExtra?448:200)}}rpx;'></view></block></block><template is='chat-input' data='{{globalData:globalData,inputObj:inputObj,textMessage:textMessage,showVoicePart:false}}'></template>