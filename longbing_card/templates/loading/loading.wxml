<template name='loading'><view class='page'><block wx:if='{{loading}}'><view class='loadmore'><view class='loading'></view><view class='loadmore_tips'>正在加载</view></view></block><block wx:else><block wx:if='{{ (page >= total_page && list.length > 0) }}'><view class='loadmore loadmore_line'><view class='loadmore_tips loadmore_tips_in-line'>没有更多数据了</view></view></block><block wx:if='{{list.length <= 0}}'><view class='loadmore loadmore_line'><view class='loadmore_tips loadmore_tips_in-line'>没有找到数据</view></view></block></block></view></template>