<template name='setTabActive'><form bindsubmit='formSubmit' class='setTab-box-view' reportSubmit='true'><view class='swiper-tab'><block wx:for='{{tabList}}' wx:for-item='item' wx:for-index='index' wx:key='index'><button class='swiper-tab-list {{(currentIndex == item.type?"active":"")}}' data-index='{{index}}' data-status='{{item.status}}' data-type='{{item.type}}' formType='submit' hoverClass='none'>        {{(item.name?item.name:item)}}        <block wx:if='{{item.dotNum > 0}}'><view class=' {{(item.dotNum >= 100?"dotbig":"dot")}}'>{{item.dotNum}}</view></block></button></block></view></form></template><template name='setTabCurrent'><form bindsubmit='formSubmit' class='setTab-box-view' reportSubmit='true'><view class='swiper-tab-curr'><block wx:for='{{tabList}}' wx:for-item='item' wx:for-index='index' wx:key='index'><button class='swiper-tab-curr-list {{(currentIndex == index?"active":"")}}' data-index='{{index}}' data-status='{{item.status}}' data-type='{{item.type}}' formType='submit' hoverClass='none'>        {{(item.name?item.name:item)}}        <block wx:if='{{item.dotNum > 0}}'><view class=' {{(item.dotNum >= 100?"dotbig":"dot")}}'>{{item.dotNum}}</view></block></button></block></view></form></template><template name='setTabBarCover'><cover-view class='{{(globalData.isIphoneX?"isIphoneX":"")}}' style='height:110rpx;'></cover-view><cover-view class='common-footer {{(globalData.isIphoneX?"isIphoneX":"")}}'><block wx:for='{{tabBarList}}' wx:for-item='item' wx:for-index='index' wx:key='key'><cover-view bindtap='toJump' class='tab-view' data-index='{{index}}' data-status='toTabBar' data-text='{{item.text}}' data-type='{{item.type}}' data-url='{{item.url}}'><cover-image src='{{(currentTabBar == item.type?item.selectedIconPath:item.iconPath)}}'></cover-image><cover-view class='{{(currentTabBar == item.type?"active":"")}}'> {{item.text}}</cover-view></cover-view></block></cover-view></template><template name='setTabMenu'><cover-view class='{{(globalData.isIphoneX?"isIphoneX":"")}}' style='height:110rpx;'></cover-view><cover-view class='common-footer {{(globalData.isIphoneX?"isIphoneX":"")}}'><block wx:for='{{globalData.tabBarList}}' wx:for-item='item' wx:for-index='index' wx:key='key'><block wx:if='{{item.showTab == 1}}'><block wx:if='{{item.jump == "toMiniApp"}}'><navigator appId='{{item.toMiniApp[0]}}' class='tab-view' data-index='{{index}}' data-status='toTabBar' extraData='{{(item.toMiniApp[2]?item.toMiniApp[2]:"")}}' hoverClass='changeHover' openType='navigate' path='{{(item.toMiniApp[1]?item.toMiniApp[1]:"")}}' target='miniProgram' version='release'><cover-image src='{{item.iconPath}}'></cover-image><cover-view class=''>{{item.text}}</cover-view></navigator></block><block wx:else><cover-view catch:tap='toSetTabMenu' class='tab-view' data-index='{{index}}' data-status='toTabBar' data-text='{{item.text}}' data-type='{{item.type}}' data-url='{{item.url}}'><block wx:if='{{item.jump == "toOutUrl"}}'><cover-image src='{{item.iconPath}}'></cover-image><cover-view class=''>{{item.text}}</cover-view></block><block wx:if='{{item.jump == "toPageUrl"}}'><cover-image src='{{(currentTabBar == item.type?item.selectedIconPath:item.iconPath)}}'></cover-image><cover-view class='{{(currentTabBar == item.type?"active":"")}}'>{{item.text}}</cover-view></block></cover-view></block></block></block></cover-view></template>