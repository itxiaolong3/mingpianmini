<template name='cardType1'>
    <view style='background:white;padding:15rpx 0rpx;'>
        <view class='card-index-sec br-10'>
            <image mode='aspectFill'
                   src='{{(cardIndexData.info.avatar_2?cardIndexData.info.avatar_2:globalData.defaultUserImg)}}'></image>
            <view class='user-card-info rel'>
                <image class='circle-img abs' src='/longbing_card/resource/images/circle.png'></image>
                <!--<image class='abs company-logo' mode='scaleToFill'-->
                <!--src='{{(cardIndexData.info.myCompany.logo?cardIndexData.info.myCompany.logo:globalData.logoImg)}}'></image>-->
                <!--<image class='abs company-logo' mode='scaleToFill'-->
                       <!--src='{{(cardIndexData.info.companylogo?cardIndexData.info.companylogo:globalData.logoImg)}}'></image>-->
                <view class='tc name ellipsis' style="padding-top: 15px; !important;">{{(cardIndexData.info.name?cardIndexData.info.name:"姓名")}}</view>
                <!--<view class='tc ellipsis'>{{(cardIndexData.info.job_name?cardIndexData.info.job_name:"职称")}}</view>-->
                <view class='tc ellipsis'>{{(cardIndexData.info.job?cardIndexData.info.job:"职称")}}</view>
                <view class='tc ellipsis'>
                    {{(cardIndexData.info.phone?cardIndexData.info.phone:"电话")}}
                </view>
                <view class='tc ellipsis'>
                    <!--{{(cardIndexData.info.myCompany.name?cardIndexData.info.myCompany.name:"公司名称")}}-->
                    {{(cardIndexData.info.company?cardIndexData.info.company:"公司名称")}}
                </view>
                <view class='tc ellipsis'>
                    {{(cardIndexData.info.company?cardIndexData.info.company2:"公司名称2")}}
                </view>
            </view>
        </view>
    </view>
</template><template name='cardType2'>
