<template name='companyTmp1'><block wx:if='{{item.list.length > 0}}'><view class='company-news-sec card-product-sec'><block wx:for='{{item.list}}' wx:for-item='item1' wx:for-index='index1' wx:key='key1'><view bindtap='toJump' class='child rel' data-id='{{item1.id}}' data-index='{{index}}' data-status='toDetail'><image class='abs br-5' mode='aspectFill' src='{{(item1.cover?item1.cover:bannerDefault)}}'></image><view class='content'><view class='title clamp2'>{{item1.title}}</view><view class='flex'><view class='flex100-6' style='flex:0 0 100%;'>{{item1.create_time2}}</view></view></view></view></block></view></block></template><template name='companyTmp2'><view class='company-introduce-sec'><htmltowxml bindWxmlTagATap='wxmlTagATap' json='{{item.info.content}}' showLoading='{{false}}'></htmltowxml></view></template><template name='companyTmp3'><block wx:if='{{item.list.length > 0}}'><view class='company-recruit-sec'><block wx:for='{{item.list}}' wx:for-item='item1' wx:for-index='index1' wx:key='key1'><view bindtap='toJump' class='child rel br-10' data-id='{{item1.id}}' data-index='{{index}}' data-status='toDetail'><view class='job ellipsis'>{{item1.title}}</view><view class='content ellipsis'>薪资:{{item1.money}}  经验:{{item1.experience}}  学历:{{item1.education}}</view><i class='iconfont icon-right2 abs'></i></view></block></view></block></template><template name='companyTmp4'><block wx:if='{{item.info}}'><view class='company-recruit-sec'><block wx:if='{{ (item.info.name1 && item.info.phone1) }}'><view bindtap='toJump' class='child contact-us rel br-10' data-content='{{item.info.phone1}}' data-status='toCall' style='margin-top:20rpx;'><view class='job ellipsis'>{{item.info.name1}}</view><view class='content ellipsis'>{{item.info.phone1}}</view><i class='iconfont icon-right2 abs'></i></view></block><block wx:if='{{ (item.info.name2 && item.info.phone2) }}'><view bindtap='toJump' class='child contact-us rel br-10' data-content='{{item.info.phone2}}' data-status='toCall' style='margin-top:0rpx;'><view class='job ellipsis'>{{item.info.name2}}</view><view class='content ellipsis'>{{item.info.phone2}}</view><i class='iconfont icon-right2 abs'></i></view></block><block wx:if='{{ (item.info.name3 && item.info.phone3) }}'><view bindtap='toJump' class='child contact-us rel br-10' data-content='{{item.info.phone3}}' data-status='toCall' style='margin-top:0rpx;'><view class='job ellipsis'>{{item.info.name3}}</view><view class='content ellipsis'>{{item.info.phone3}}</view><i class='iconfont icon-right2 abs'></i></view></block><block wx:if='{{item.info.address}}'><view bindtap='toJump' class='address rel' data-content='{{item.info.address}}' data-latitude='{{item.info.latitude}}' data-longitude='{{item.info.longitude}}' data-status='toCompanyMap'><i class='iconfont icon-dizhi abs'></i>            {{item.info.address}}        </view></block></view></block></template><template name='companyTmp5'><block wx:if='{{item.list.length > 0}}'><view class='company-team-sec card-product-sec'><block wx:for='{{item.list}}' wx:for-item='item1' wx:for-index='index1' wx:key='key1'><view bindtap='toJump' class='child rel br-10' data-id='{{item1.id}}' data-index='{{index}}' data-status='toDetail'><image class='abs' mode='aspectFill' src='{{(item1.cover?item1.cover:bannerDefault)}}'></image><view class='content'><view class='name ellipsis'>{{item1.name}}</view><view class='job ellipsis'>{{item1.job}}</view><view class='job-before-sec'><block wx:if='{{item1.experience1}}'><view class='ellipsis'>                            {{item1.experience1}}                        </view></block><block wx:if='{{item1.experience2}}'><view class='ellipsis'>                            {{item1.experience2}}                        </view></block><block wx:if='{{item1.experience3}}'><view class='ellipsis'>                            {{item1.experience3}}                        </view></block></view></view></view></block></view></block></template><template name='companyTmp7'><block wx:if='{{item.list.length > 0}}'><view class='company-video-sec card-product-sec'><block wx:for='{{item.list}}' wx:for-item='item1' wx:for-index='index1' wx:key='key1'><view bindtap='toJump' class='child rel' data-content='/longbing_card/users/pages/company/detail/detail?status=toPlayVideo&name={{item1.title}}&src={{item1.video}}&id={{item1.id}}&table_name={{item.table_name}}&to_uid={{to_uid}}' data-shareimg='{{(item1.cover?item1.cover:companyVideoImg)}}' data-status='toPlayVideo'><view class='title'>{{item1.title}}</view><image class='abs cover-img' mode='aspectFill' src='{{(item1.cover?item1.cover:companyVideoImg)}}'></image><view class='video-bg abs bg_opacity_sec tc' style='z-index:9999;'><image class='abs play-img' mode='aspectFill' src='{{playVideoImg}}'></image></view></view></block></view></block></template><template name='companyTmp8'><view class='company-leavmessage-sec'><view class='leavmessage br-10'><view class='title tc ellipsis rel'>{{item.name}}</view><view class='text tc ellipsis'>{{item.remark}}</view><form bindsubmit='formTmpSubmit' class='content clearfix' reportSubmit='true'><input class='text br-10' name='name' placeholder='请填写您的姓名' type='text'></input><input class='text br-10' name='phone' placeholder='请填写您的联系电话' type='text'></input><input class='text br-10' name='content' placeholder='请填写您想说的话' type='text'></input><view class='clearfix'></view><button class='btn br-10' data-index='{{index}}' data-status='toFormSubmit' formType='submit' hoverClass='none'>立即留言</button></form></view></view></template>